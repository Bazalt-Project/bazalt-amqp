    "url": "https://github.com/Bazalt-Project/bazalt-amqp/issues"
  },
  "homepage": "https://github.com/Bazalt-Project/bazalt-amqp#readme",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.5.1",
    "babel-preset-es2015": "^6.5.0",
